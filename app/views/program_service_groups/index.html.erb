<h1>Program Service Groups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @program_service_groups.each do |program_service_group| %>
      <tr>
        <td><%= link_to 'Show', program_service_group %></td>
        <td><%= link_to 'Edit', edit_program_service_group_path(program_service_group) %></td>
        <td><%= link_to 'Destroy', program_service_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

